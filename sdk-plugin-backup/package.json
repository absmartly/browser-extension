{
  "name": "@absmartly/dom-changes-plugin",
  "version": "0.0.1",
  "description": "ABSmartly SDK plugin for applying DOM changes from experiment variants",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --clean --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix"
  },
  "peerDependencies": {
    "@absmartly/javascript-sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "absmartly",
    "ab-testing",
    "dom-manipulation",
    "experimentation"
  ],
  "author": "ABSmartly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/absmartly/dom-changes-plugin"
  }
